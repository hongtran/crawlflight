<div class="row flight-results-header-wrapper">
  <%= link_to nil, "data-toggle" => "modal", "data-target" => "#alert_price"  do %>
    <div class="col-sm-1 flight-results-header bell">
    <i class="fa fa-bell" aria-hidden="true"></i>
    </div>
  <% end %>
  <%= render "alert", :locals => {:ori_airport_id => @ori_airport_id, :des_airport_id => @des_airport_id, :from_date => @from_date, :to_date => @to_date} %>
  <div class="col-sm-10 col-sm-offset-1 flight-results-header select-header">
    <div class="round-type">Select Departure</div>
    <div class="date">SGN <i class="fa fa-plane" aria-hidden="true"></i> BMV, Thu, 22/11/2016</div>
  </div>
</div>
<div class="flight-results-wrapper container">
