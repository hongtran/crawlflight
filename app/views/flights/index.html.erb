    <div class="col-sm-12 price-info-bar">
      Pricing is include ticket price and all taxes for all passengers      
    </div>
  </div>
  <div class="row flight-result">
    <div class="col-xs-3">
      <%= image_tag("airlines/vjet.png", alt: "Vietjet Air") %>
    </div>
    <div class="col-xs-6">
      <div class="depart">
        <span class="time">15:30</span>
        <span class="stop-station">SGN</span>
      </div>
      <div class="arrow-right"><i class="fa fa-long-arrow-right" aria-hidden="true"></i></div>
      <div class="arrive">
        <span class="time">16:30</span>
        <span class="stop-station">BMV</span>
      </div>
    </div>
    <div class="col-xs-3">
      <button type="button" class="btn btn-primary price">
        12.000 VND
      </button>
    </div>
  </div>
